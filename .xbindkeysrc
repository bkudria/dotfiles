#keystate_numlock = enable
#keystate_scrolllock = enable
#keystate_capslock = enable

"keyout {left}"
  b:6

"keyout {right}"
  b:7

"dcop kicker kicker toggleShowDesktop"
	b:8

"toggle-kmail"
	b:9

"xte "key XF86AudioLowerVolume""
	b:15

"xte "key XF86AudioRaiseVolume""
	b:13

"keyout "{alt}1{alt}""
	b:17

"keyout {F6}"
  Control + b:6

"keyout {F6}"
  Control + b:7
